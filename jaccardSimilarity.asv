num_components_iter3=size(tripcomp_3_sub,1);
jaccard_3rdIter = zeros(num_components_iter3,num_components_iter3);
for i=1:num_components_iter3
    for j=i+1:num_components_iter3
        [x,subi,z]=find(tripcomp_3_sub(i,:));
        [x,subj,z]=find(tripcomp_3_sub(j,:));
        
        jaccard_3rdIter(i,j) = intersect(subi,subj)/union(subi,subj) + intersect(subi,subj)/union(subi,subj)+intersect(subi,subj)/union(subi,subj);
    end
end